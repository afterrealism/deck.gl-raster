{
  "name": "@afterrealism/deck.gl-raster",
  "version": "0.1.3",
  "description": "GPU-accelerated Cloud-Optimized GeoTIFF visualization in deck.gl (Web Mercator only)",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./gpu-modules": {
      "types": "./dist/gpu-modules/index.d.ts",
      "default": "./dist/gpu-modules/index.js"
    },
    "./affine": {
      "types": "./dist/reproject/affine/index.d.ts",
      "default": "./dist/reproject/affine/index.js"
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "build:watch": "tsc --build tsconfig.build.json --watch",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "deck.gl",
    "geotiff",
    "cog",
    "cloud-optimized-geotiff",
    "raster",
    "visualization",
    "webgl",
    "gpu"
  ],
  "author": "Development Seed",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/afterrealism/deck.gl-raster.git"
  },
  "devDependencies": {
    "@luma.gl/core": "9.2.5",
    "@luma.gl/shadertools": "9.2.5",
    "@types/node": "^25.0.3",
    "jsdom": "^27.4.0",
    "publint": "^0.3.16",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  },
  "peerDependencies": {
    "@deck.gl/core": "^9.2.5",
    "@deck.gl/geo-layers": "^9.2.5",
    "@deck.gl/layers": "^9.2.5",
    "@deck.gl/mesh-layers": "^9.2.5",
    "@luma.gl/core": "^9.2.5",
    "@luma.gl/shadertools": "^9.2.5"
  },
  "dependencies": {
    "@math.gl/core": "^4.1.0",
    "@math.gl/culling": "^4.1.0",
    "@math.gl/web-mercator": "^4.1.0",
    "geotiff": "^2.1.3"
  },
  "pnpm": {
    "overrides": {
      "@luma.gl/constants": "9.2.5",
      "@luma.gl/core": "9.2.5",
      "@luma.gl/engine": "9.2.5",
      "@luma.gl/gltf": "9.2.5",
      "@luma.gl/shadertools": "9.2.5",
      "@luma.gl/webgl": "9.2.5"
    }
  },
  "packageManager": "pnpm@10.25.0",
  "volta": {
    "node": "24.11.1",
    "pnpm": "10.25.0"
  }
}
